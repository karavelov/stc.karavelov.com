from django.contrib.auth.hashers import make_password
from cmsapp.models import CustomUser
new_password = "123"  # –ü–∞—Ä–æ–ª–∞—Ç–∞, –∫–æ—è—Ç–æ —â–µ —Å–µ –∑–∞–¥–∞–¥–µ –Ω–∞ –≤—Å–∏—á–∫–∏ –ø–æ—Ç—Ä–µ–±–∏—Ç–µ–ª–∏
