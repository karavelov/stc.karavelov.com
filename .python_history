from django.contrib.auth.hashers import make_password, identify_hasher
from cmsapp.models import CustomUser
users_updated = 0
for user in CustomUser.objects.all():
    if not user.password.startswith('bcrypt$'):  # –ü—Ä–æ–≤–µ—Ä—è–≤–∞–º–µ –¥–∞–ª–∏ –ø–∞—Ä–æ–ª–∞—Ç–∞ –µ –≤–µ—á–µ bcrypt
        try:
            hasher = identify_hasher(user.password)  # –ü—Ä–æ–≤–µ—Ä—è–≤–∞–º–µ –¥–∞–ª–∏ –ø–∞—Ä–æ–ª–∞—Ç–∞ –µ –≤–∞–ª–∏–¥–Ω–∞
            user.password = make_password(user.password)  # –ü—Ä–µ-—Ö–µ—à–∏—Ä–∞–Ω–µ —Å bcrypt
            user.save()
            users_updated += 1
        except Exception as e:
            print(f"–ì—Ä–µ—à–∫–∞ –ø—Ä–∏ –æ–±–Ω–æ–≤—è–≤–∞–Ω–µ –Ω–∞ {user.username}: {e}")
print(f"‚úÖ –û–±–Ω–æ–≤–µ–Ω–∏ {users_updated} –ø–∞—Ä–æ–ª–∏ –¥–æ bcrypt!")
